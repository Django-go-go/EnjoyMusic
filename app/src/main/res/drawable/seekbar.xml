    <item android:id="@+id/background">
        <shape>
            <solid android:color="#f4f3f6" />
        </shape>
    </item>
    <item android:id="@+id/secondaryProgress">
        <clip>
            <shape>
                <solid android:color="#f5f3f7" />
            </shape>
        </clip>
    </item>
    <item android:id="@+id/progress">
        <clip>
            <shape>
                <solid android:color="#e30c22" />
            </shape>
        </clip>
    </item>
</layer-list>
